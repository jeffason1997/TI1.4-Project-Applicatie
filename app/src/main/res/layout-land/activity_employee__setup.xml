<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.b2.projectgroep.ti14_applicatie.EmployeeClasses.Employee_SetupActivity"
    tools:layout_editor_absoluteY="73dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="128dp"
        android:layout_height="41dp"
        android:text="@string/Echoose_cardnumber"
        android:textSize="20sp"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/employee_setup_cardnumber"
        android:layout_marginTop="8dp"
        android:layout_marginStart="32dp" />

    <EditText
        android:id="@+id/employee_setup_cardnumber"
        android:layout_width="215dp"
        android:layout_height="43dp"
        android:layout_marginRight="32dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="66dp"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/employee_setup_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="65dp"
        android:layout_marginEnd="5dp"
        android:text="@string/Echoose_confirm_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/employee_setup_cardnumber"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1" />
</android.support.constraint.ConstraintLayout>
