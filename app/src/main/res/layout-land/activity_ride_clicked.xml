<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.b2.projectgroep.ti14_applicatie.RideClasses.PersonalActivityDetailedInformation"
    tools:layout_editor_absoluteY="73dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="849dp"
        android:layout_height="59dp"
        app:srcCompat="@color/colorGreen"
        tools:layout_editor_absoluteX="-33dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ride_detailedTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="18dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginRight="208dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/ride_detailedImage"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="208dp" />

    <ImageView
        android:id="@+id/ride_detailedImage"
        android:layout_width="185dp"
        android:layout_height="134dp"
        app:srcCompat="@drawable/python"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="48dp"
        android:layout_marginTop="78dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="48dp" />

    <TextView
        android:id="@+id/ride_detailedDescription"
        android:layout_width="208dp"
        android:layout_height="0dp"
        android:text="TextView"
        android:layout_marginStart="77dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="74dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="78dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/ride_detailedImage" />

    <TextView
        android:id="@+id/ride_detaildedTime"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:text="TextView"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView12"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/ride_detailedImage"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lv_kind_tijd"
        app:layout_constraintTop_toBottomOf="@+id/ride_detailedImage"
        android:layout_marginLeft="48dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="48dp" />

</android.support.constraint.ConstraintLayout>
